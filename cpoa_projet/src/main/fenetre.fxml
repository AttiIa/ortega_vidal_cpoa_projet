            <Button alignment="CENTER" mnemonicParsing="false" text="Periodicite">
               <FlowPane.margin>
                  <Insets left="25.0" right="45.0" top="100.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" text="Client">
               <FlowPane.margin>
                  <Insets right="45.0" top="100.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" text="Abonnement">
               <FlowPane.margin>
                  <Insets right="45.0" top="100.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" text="Revue">
               <FlowPane.margin>
                  <Insets top="100.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
</VBox>
